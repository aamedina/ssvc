:SPDXSpecification a owl:NamedIndividual ;
  rdfs:label "SPDX" ;
  rdfs:comment "SPDXÂ® is an open standard for communicating software bill of material information, including components, licenses, copyrights, and security references. SPDX reduces redundant work by providing a common format for companies and communities to share important data, thereby streamlining and improving compliance." ;
  dcterms:issued "2023-07-31T14:53:02Z"^^xsd:dateTime ;
  dcterms:modified "2023-07-31T14:53:02Z"^^xsd:dateTime ;
  dcterms:license <http://spdx.org/licenses/Community-Spec-1.0> ;
  dcterms:hasPart spdx-core:ProfileIdentifierType-AI ;
  dcterms:hasPart spdx-core:ProfileIdentifierType-Build ;
  dcterms:hasPart spdx-core:ProfileIdentifierType-Core ;
  dcterms:hasPart spdx-core:ProfileIdentifierType-Dataset ;
  dcterms:hasPart spdx-core:ProfileIdentifierType-ExpandedLicensing ;
  dcterms:hasPart spdx-core:ProfileIdentifierType-Security ;
  dcterms:hasPart spdx-core:ProfileIdentifierType-SimpleLicensing ;
  dcterms:hasPart spdx-core:ProfileIdentifierType-Software ;
  owl:versionInfo "3.0"^^xsd:string .

